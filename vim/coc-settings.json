{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "sql.formatOptions": {
    "linesBetweenQueries": 2
  },
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "eslint.autoFixOnSave": true,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["typescript", "javascriptreact"]
  },
  "snippets.excludePatterns": ["$HOME/.config/coc/extensions/node_modules/**"],
  "coc.preferences.snippetHighlight": true,
  "colors.filetypes": ["css", "html"],
  "javascript.disableProgressNotifications": true,
  "typescript.disableProgressNotifications": true,
  "rust-analyzer.disableProgressNotifications": true,
  "go.disableProgressNotifications": true
}
